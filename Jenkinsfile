pipeline {
    // 1行コメント
    /*
     * 複数行コメント
     */
    stages {
        stage("-------SCM checkout-------"){
            steps{
                echo 'helloworld'
            }
        }

        stage("-------構文チェック-------"){
            steps{
                echo 'helloworld'
            }
        }

         stage("-------lint-------"){
            steps{
                echo 'helloworld'
            }
        }
    }
}
